<h1 style="margin:40px 40px;">KeyWord</h1> {{#each this}}{{#each this}}
<div class="row">
    <div class="col-md-4 col-sm-4 col-xs-12">
        <h3 class="hidden-xs" style="font-weight:200;text-align:right;margin-top:0px;"> {{@key}} </h3>
        <h3 class="hidden-lg hidden-md hidden-sm" style="font-weight:200;text-align:left;margin-top:0px; margin-left:15px;"> {{@key}} </h3>
    </div>
    <div class="col-md-8 col-sm-8 col-xs-12">
        {{#each this}}
        <div class="row" style="margin-top:10px;">
            <div class="col-md-5 col-sm-5 col-xs-5">
                <p style="font-size:1.3em;padding:0px 20px">{{cvTermName}}</p>
            </div>
            <div class="col-md-1 col-sm-2 col-xs-1">
                <button class="btn btn-info" type="button" data-toggle="collapse" data-target="#{{cvTermAccessionCode}}" aria-expanded="false" aria-controls="{{cvTermAccessionCode}}">Definition</button>
            </div>
            <div class="col-md-3 col-sm-2 col-sm-offset-1 col-xs-1 col-xs-offset-1">
                <a class="btn" style="color:#C50063;border:1px solid #C50063;margin-left:20px;">{{cvTermAccessionCode}}</a>
            </div>
        </div>
        <div class="collapse" id="{{cvTermAccessionCode}}">
            <div class="well" style="background-color:rgba(91, 192, 222, 0.2);margin-right:50px;margin-left:10px;margin-top:10px;">
                {{cvTermDescription}}
            </div>
        </div>
        {{/each}}
    </div>
</div>
<hr style="margin:10px 50px"> {{/each}}{{/each}}