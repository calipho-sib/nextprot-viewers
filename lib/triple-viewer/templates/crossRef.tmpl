<div id="crossRefTitle">
    <h2>Further external links</h2>
</div>

<div id="crossRefLinks">
    <div class="row">
        <div class="col-md-7 col-xs-6">
            {{#each type}}
            <dl>
                <dt>{{@key}}</dt>
                {{#this}}
                <dd>{{dBName}} : {{{link_to}}}</dd>
                {{/this}}
            </dl>

            {{/each}}
        </div>

    </div>
</div>